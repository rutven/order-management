<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity OrderPosition.
    -->
    <changeSet id="20180927191034-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="order_position"
                                 constraintName="fk_order_position_product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

        <addForeignKeyConstraint baseColumnNames="order_entity_id"
                                 baseTableName="order_position"
                                 constraintName="fk_order_position_order_entity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="order_entity"/>

    </changeSet>
</databaseChangeLog>
